<td class="selection"><input type="checkbox" <% if(selected){ %> checked="checked" <% } %>/></td>
<% _.each(activeColumns, function(column, index){ %>
  <% if(index == 0) { %>
    <td class="structure-first-column <%- column %>">
      <a href="<%- getURL %>">
        <% if(attributes.is_folderish) { %>
          <span class="glyphicon glyphicon-folder-open"></span>
        <% } else if(iconMapping.hasOwnProperty(attributes.Type)) { %>
          <span class="glyphicon glyphicon-<%- iconMapping[attributes.Type] %>"></span>          
        <% } %>      
        <%- attributes[column] %>
      </a>
    </td>    
  <% } else { %>
    <% if(_.has(availableColumns, column)) { %>
        <td class="<%- column %>">
          <% if(attributes[column] instanceof Array) { %>
            <span><%= attributes[column].join('</span>, <span>') %></span>
          <% } else { %>
            <%- attributes[column] %>
          <% } %>
        </td>
    <% } %>
  <% } %>
<% }); %>
<td class="actionmenu-container">
</td>
